<h1>新規論文投稿</h1>
<%= form_with model: @article, local: true do |f| %>
  <div class = "form-group">
    <div class = "form-control">
      <p><%= f.label "論文タイトル" %></p>
      <p><%= f.text_field :title %></p>
    </div>

    <div class = "form-control">
      <p>><%= f.label "本文" %></p>
      <p><%= f.text_area :body %></p>
    </div>

    <%= f.submit "投稿" %>
  </div>
<% end %>



